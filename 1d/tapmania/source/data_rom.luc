module data_rom (
    input a[8],
    output d[32]
  ){

  always {
    case (a) {
      1: d = 32d20;     // MAX_STAGE = 20
      2: d = 32d18000;  // MAX_TIME = 3 mins
      3: d = 32d100;    // FAIL_DELAY = 1s
      4: d = 32d30;     // PASS_DELAY = 0.3s
      5: d = 32d300;    // HOLD_DELAY = 3s
      128: d = b00000000000100010010100000000000;   // stage 1:  XXYYBRXX
      129: d = b00000000100000100010000100000000;   // stage 2:  XXRBBYXX
      130: d = b00000000000101000100100000000000;   // stage 3:  XXYGGRXX
      131: d = b00000000000110000001100000000000;   // stage 4:  XXYRYRXX
      132: d = b00000000001000011000001000000000;   // stage 5:  XXBYRBXX
      133: d = b00000000010010000100001000000000;   // stage 6:  XXGRGBXX
      134: d = b00000100000101000001100000100000;   // stage 7:  XGYGYRBX
      135: d = b00000001001000100100010010000000;   // stage 8:  XYBBGGRX
      136: d = b00000001000101000001010010000000;   // stage 9:  XYYGYGRX
      137: d = b00001000010010000100100000100000;   // stage 10: XRGRGRBX
      138: d = b00000001100001000001001000100000;   // stage 11: XYRGYBBX
      139: d = b00000001000100100001100000010000;   // stage 12: XYYBYRYX
      140: d = b00000010100001000010100010000000;   // stage 13: XBRGBRRX
      141: d = b10000010000100010001010010000001;   // stage 14: RBYYYGRY
      142: d = b10000100010010000100001010000010;   // stage 15: RGGRGBRB
      143: d = b01001000010000011000000100100010;   // stage 16: GRGYRYBB
      144: d = b00010010000100100001010001001000;   // stage 17: YBYBYGGR
      145: d = b10001000010000010001001000100010;   // stage 18: RRGYYBBB
      146: d = b00101000000101001000001001001000;   // stage 19: BRYGRBGR
      147: d = b01000001010000100010001010000001;   // stage 20: GYGBBBRY
      default: d = 0;
    }
  }
}
